@model WEB_T04_Team6.Models.VoucherViewModel

@{
    ViewData["Title"] = "Details";
}
<body class="bg-light">
<h4 class="PageTitle">View Staff Details</h4>
<hr />
<fieldset class="FormContent">
    <div class="form-group row">
        <label asp-for="IssuingID"
               class="col-sm-3 col-form-label"></label>
        <div class="col-sm-9 form-control-plaintext">
            @Html.Display("IssuingID")
        </div>
    </div>
    <div class="form-group row">
        <label asp-for="MemberID" class="col-sm-3 col-form-label"></label>
        <div class="col-sm-9 form-control-plaintext">
            @Html.Display("MemberID")
        </div>
    </div>
    <div class="form-group row">
        <label asp-for="Amount" class="col-sm-3 col-form-label"></label>
        <div class="col-sm-9 form-control-plaintext">
            @Html.Display("Amount")
        </div>
    </div>
    <div class="form-group row">
        <label asp-for="MonthIssuedFor"
               class="col-sm-3 col-form-label"></label>
        <div class="col-sm-9 form-control-plaintext">
            @Html.Display("MonthIssuedFor")
        </div>
    </div>
    <div class="form-group row">
        <label asp-for="YearIssuedFor" class="col-sm-3 col-form-label"></label>
        <div class="col-sm-9 form-control-plaintext">
            @Html.Display("YearIssuedFor")
        </div>
    </div>
    <div class="form-group row">
        <label asp-for="DateTimeIssued" class="col-sm-3 col-form-label"></label>
        <div class="col-sm-9 form-control-plaintext">
            @Html.Display("DateTimeIssued")
        </div>
    </div>
    <div class="form-group row">
        <label asp-for="VoucherSN"
               class="col-sm-3 col-form-label"></label>
        <div class="col-sm-9 form-control-plaintext">
            @Html.Display("VoucherSN")
        </div>
    </div>
    <div class="form-group row">
        <label asp-for="Status" class="col-sm-3 col-form-label"></label>
        <div class="col-sm-9 form-control-plaintext">
            @Html.Display("Status")
        </div>
    </div>
    <div class="form-group row">
        <label asp-for="DateTimeRedeemed" class="col-sm-3 col-form-label"></label>
        <div class="col-sm-9 form-control-plaintext">
            @Html.Display("DateTimeRedeemed")
        </div>
    </div>

    <div class="form-group row">
        <div class="col-sm-9 offset-sm-3">
            <a asp-action="Index"
               asp-controller="Voucher">Back to List</a>
        </div>
    </div>
</fieldset>
</body>