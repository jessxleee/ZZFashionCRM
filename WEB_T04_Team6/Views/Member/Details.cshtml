@model WEB_T04_Team6.Models.MemberViewModel

@{
    ViewData["Title"] = "Details";
}


<h4 class="PageTitle">View Details</h4>
<hr />
<body class="bg-light">
    <fieldset class="FormContent">
        <div class="form-group row">
            <label asp-for="MemberID"
                   class="col-sm-3 col-form-label"></label>
            <div class="col-sm-9 form-control-plaintext">
                @Html.Display("MemberID")
            </div>
        </div>
        <div class="form-group row">
            <label asp-for="Name" class="col-sm-3 col-form-label"></label>
            <div class="col-sm-9 form-control-plaintext">
                @Html.Display("Name")
            </div>
        </div>
        <div class="form-group row">
            <label asp-for="Gender" class="col-sm-3 col-form-label"></label>
            <div class="col-sm-9 form-control-plaintext">
                @Html.Display("Gender")
            </div>
        </div>

        <div class="form-group row">
            <label asp-for="BirthDate" class="col-sm-3 col-form-label"></label>
            <div class="col-sm-9 form-control-plaintext">
                @Html.Display("BirthDate")
            </div>
        </div>
        <div class="form-group row">
            <label asp-for="Address"
                   class="col-sm-3 col-form-label"></label>
            <div class="col-sm-9 form-control-plaintext">
                @Html.Display("Address")
            </div>
        </div>
        <div class="form-group row">
            <label asp-for="Country"
                   class="col-sm-3 col-form-label"></label>
            <div class="col-sm-9 form-control-plaintext">
                @Html.Display("Country")
            </div>
        </div>
        <div class="form-group row">
            <label asp-for="TelNo" class="col-sm-3 col-form-label"></label>
            <div class="col-sm-9 form-control-plaintext">
                @Html.Display("TelNo")
            </div>
        </div>
        <div class="form-group row">
            <label asp-for="EmailAddr" class="col-sm-3 col-form-label"></label>
            <div class="col-sm-9 form-control-plaintext">
                @Html.Display("EmailAddr")
            </div>
        </div>


        <div class="form-group row">
            <div class="col-sm-9 offset-sm-3">
                <a asp-action="Index"
                   asp-controller="Member">Back to List</a>
            </div>
        </div>
    </fieldset>
    </body>